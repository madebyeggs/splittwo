.passwordContent {
	position: fixed;
	top: 50%;
	left: 50%;
	 /* bring your own prefixes */
	transform: translate(-50%, -50%);
	width:320px;
}
.fa-lock {
	position:absolute;
	width:45px;
	margin:-80px auto 0px auto!important;
	padding-left:137.5px;
}
.passwordContent > input {-webkit-text-security:disc;height:48px;width:320px;color:#3a3a3a;padding:17px 48px 14.5px 15px;font-family:'SplitCustomGotham';}
.fa.dark {color:#3a3a3a;position:absolute;right:15px;top:27px;display:none;}
.error {
	position: fixed;
	top: 50%;
	left: 50%;
	 /* bring your own prefixes */
	transform: translate(-50%, -50%);
	width:320px;
	margin-top:66px;
	background-color:#ff0000;color:#fff;font-family:"SplitCustomGotham";width:320px;height:48px;text-align:center;text-transform:uppercase;padding-top:12.5px;
	display:none;
}

.projectlist a:hover { 
	cursor: url('	'), crosshair;
	cursor: url(<%= asset_path 'more.png' %>) 40 40, crosshair;
}

.projectlist a:hover .projectinfo { 
	background: #202326; 
	-webkit-transition: background .25s linear;
	-moz-transition: background .25s linear;
	-o-transition: background .25s linear;
	transition: background .25s linear; 
}
.overlay, .overlay2 {
    position:absolute;
	top:0px; 
	width:100%; 
	height:100%;
	margin:2px; 
	background:#202326; 
	opacity:0;
}
.overlay2 {background:#ff0000;opacity:0.4;}
.project:hover .overlay {
	opacity: 1;
	transition: opacity .25s ease-in-out;
	-moz-transition: opacity .25s ease-in-out;
	-webkit-transition: opacity .25s ease-in-out;
}

.customName {text-align:left;margin:30px 0px 22px 0px;}
.customSeparator {color:#ffffff; margin:54px 5%;width:90%;}
.customInfo {width:95%;margin:54px 2.5%;padding:2px 0px 40px 0px;}
.customDetail {margin:0px 0px;text-align:left;}
.customInfo p {margin:24px 0px;font-size:16px;font-family:"SplitCustomGotham";}
.customSquareImage {-webkit-filter: grayscale(100%);filter: grayscale(100%);-webkit-filter: brightness(35%);}
.custom-bottom {width:100%;clear:both;float:left;margin:30px 0px 40px 0px;}
.customCloseIcon {
	position:absolute;
	width: 48px;
	height: 48px;
	background: url("<%= asset_path 'bg-close.png' %>") center no-repeat;
	background-size:48px 48px;
	cursor: pointer;
	z-index: 1000000000;
	top:30px;
	left:30px;
}
.customBrandIcon {
	position:absolute;
	width: 48px;
	height: 48px;
	background: url(<%= asset_path 'bg-menu.png' %>) center no-repeat;
	background-size:48px 48px;
	cursor: pointer;
	z-index: 1000000000;
	top:30px;
	right:30px;
}
.customBrandIcon2, .customBrandIcon3 {
	position:absolute;
	width: 48px;
	height: 48px;
	background: url(<%= asset_path 'bg-menu.png' %>) center no-repeat;
	background-size:48px 48px;
	cursor: pointer;
	z-index: 1000000000;
	top:53%;
	right:30px;
}
.customBtn a, .customBtn a:visited {text-decoration:none;}
.subMenu {position:absolute;top:98px;right:38px;width:358px;background-color:black;z-index:2000;border-radius:1px;padding:25px;display:none;}
.subMenu2, .subMenu3 {position:absolute;top:50vh;right:0;width:33.33%;height:50vh;background-color:#000000;z-index:5000;border-radius:5px;padding:25px;display:none;}
.subMenu3 {width:50%;}
.subMenu p {margin:0;}
.videosWrapper {width:97.5%;}
.videoPositionFix {margin-left:5%;}
.responsiveVideo {width:31%;display:inline-block;}
.responsiveVideo:nth-child(2) {margin-left:2%;margin-right:2%;}
.responsiveVideo2 {width:100%;}
.frame_container {position:relative;}
.no-js .loading {display: none;}
.loading {position: absolute;top:50%;left:50%;transform: translate(-50%, -50%);}
.js .my_iframe {visibility: hidden;}
.customLogo {width:150px;position:absolute;right:30px;z-index:100000000000;top:30px;}
.customLogo img {max-width:100%;}
.customLinks {position:absolute;margin:0px 1.5px -30px 1.5px;}
.customSocialLinks {font-size:20px!important;margin-right:-5px;}
.customTitle {letter-spacing:2px;text-align:center;margin-bottom:54px;margin-top:-10px;}
.parent2, .parent3 {position:relative;}
.child2 {
	padding:0px 10px;
	width:100%;
	text-align:center;
    position: absolute;
    left: 50%;
    top: 48%;
    transform: translate(-50%, -48%);
	z-index:2000000;
}
.center {position:absolute;left:50%;top:50%;transform: translate(-50%, -50%);text-align:center;}
.marginMinify {margin:24px;}
.customDeetsh3 {margin:12px!important;}
.customDeets {margin:0px;} 
.customBtn {
  	display:inline-block;
  	text-align:center;
  	cursor:url('	'), crosshair;
	cursor:url(<%= asset_path 'more.png' %>) 40 40, crosshair;
  	border:2px solid white;
  	padding-top:44px;
  	font-size:18px;
	font-family:"SplitCustomGotham";
	text-transform:uppercase;
  	width:410px;
  	height:120px;
	margin-top:12px;
}
.customBtn:hover {
	background-color:#202326;
	-webkit-transition: background .25s linear;
	-moz-transition: background .25s linear;
	-o-transition: background .25s linear;
	transition: background .25s linear;
}
.belowTheFold {display:inline-block;width:90%;margin:0 5%;}
.aboutWrapper {width:40%;float:left;}
.customTitleAbout {letter-spacing:2px;text-align:left;margin-bottom:54px;margin-top:-10px;}
.listenWrapper {width:60%;padding-left:54px;float:right;}
.customTitleListen {letter-spacing:2px;text-align:left;margin-bottom:54px;margin-top:-10px;}
.nav-tabs{
  background-color:none;
}
.tab-content{
    background-color:none;
    color:#fff;
    padding:5px
}
.nav-tabs > li > a{
  border: medium none;
}
.nav-tabs > li > a:hover {
  background-color: #303136 !important;
    border: medium none;
    border-radius: 0;
    color:#fff;
}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus {
	background-color:#000000 !important;
}
.customBtn {width:100%;}
.noBackground {background-color:#000000!important;font-size:18px;font-style:italic;}
.relative {position:relative;}
.dummyTile {position:absolute;z-index:1;}
.brandingTile, .brandingTileSmaller {position:absolute;z-index:1000;}
.brandingTile {
	position: absolute;
	top:38%;
	left: 50%;
	transform: translate(-50%, 38%);
}
.customSocial {margin:46px 0px 0px 15px;}

@media (max-width: 767px) {
	.customInfo {width:95%;margin:34px 2.5%;padding:2px 0px 0px 0px;}
	.belowTheFold {width:100%;margin:0!important;padding:0!important;}
	.customTitle {margin-bottom:24px;}
	.videoPositionFix {width:100%;margin:0% 1%;}
	.responsiveVideo {width:100%;display:inline-block;margin:6px 0px;}
	.responsiveVideo:nth-child(2), .responsiveVideo:nth-child(3)  {display:none;}
	.customSeparator {margin:24px 1% 34px 1%;width:98%;}
	.belowTheFold {display:inline-block;width:100%;}
	.aboutWrapper {width:98%;float:left;margin:-24px 1% 0 1%;}
	.aboutWrapper p {text-align:justify;}
	.customTitleAbout {display:none;}
	.listenWrapper {width:98%;float:left;margin:12px 1%;padding-left:0;}
	.customTitleListen {display:none;}
	
}
@media (max-width:991px){
	.customBrandIcon2, .customBrandIcon3 {top:101.5%;}
	.subMenu2, .subMenu3 {top:100%;width:100%;}
}