<div class="deviseStyling col-md-12 abouts marginTopOverride canScroll">
	<div class="formContent">
		<h2 class="titles">Add your custom artist here!</h2>
		<%= bootstrap_form_for(@custom, :html => {:multipart => true}) do |f| %>
			<div class="vimeoSectionInForm">
				<div class="textField"><%= f.text_field :name, label: "Artist name", class: "" %></div>
				<div class="textField"><%= f.text_field :genre, label: "Genres (separate with /)" %></div>
				<div class="textField"><%= f.text_field :soundcloud, label: "Soundcloud playlist ID" %></div>
			</div>
			<div class="vimeoSectionInForm"><div class="textArea"><%= f.text_area :description, label: "Description" %></div></div>
			<div class="fullWidthField smallText"><%= f.check_box :display, label: "Display on Custom page" %></div>
			<div class="vimeoSectionInForm">
				<div class="textField"><%= f.file_field :image, label: "Image (crop to 1100px X 733px before upload)" %>
					<% if @custom.image.exists? %>
						<p class="existingImageName">{ Current Image: "<%= @custom.image_file_name %>" }</p>
					<% end %>
				</div>
				<div class="textField"><%= f.text_field :fullqual, label: "Add link to zipped wavs" %></div>
				<div class="textField"><%= f.text_field :lowqual, label: "Add link to zipped mp3s" %></div>
			</div>
		  	<div class="submitLarge"><%= f.submit 'Submit', class: "btn btn-success btnLarge"  %></div>
		<% end %>
	</div>
</div>