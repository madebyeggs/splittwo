<main role="main">
	<div class="col-md-12 mainContent projectlist">
		<div class="desktop">
			<% if @sorted_custom_displays.count == 7 %></h1>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/smallTile2", :c => c %>
				<% end %>
				<%= render 'smallBrandingTile2' %>
			<% elsif @sorted_custom_displays.count == 6 %></h1>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/smallTile", :c => c %>
				<% end %>
			<% elsif @sorted_custom_displays.count == 5 %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/smallTile", :c => c %>
				<% end %>
				<%= render 'smallBrandingTile' %>
			<% elsif @sorted_custom_displays.count == 4 %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/largeTile", :c => c %>
				<% end %>
			<% else %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/largeTile", :c => c %>
				<% end %>
				<%= render 'largeBrandingTile' %>
			<% end %>
		</div>
		<div class="mobile">
			<% if @custom_count == 6 %></h1>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/smallTile", :c => c %>
				<% end %>
			<% elsif @custom_count == 5 %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/smallTile", :c => c %>
				<% end %>
			<% elsif @custom_count == 4 %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/largeTile", :c => c %>
				<% end %>
			<% else %>
				<%= render 'largeBrandingTile' %>
				<% @sorted_custom_displays.each do |c| %>
					<%= render "customs/largeTile", :c => c %>
				<% end %>
			<% end %>
		</div>
	</div>
</main>