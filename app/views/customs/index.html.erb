<div class="col-md-12 mainContent projectlist">
	<% if @custom_count == 6 %></h1>
		<% @sorted_custom_displays.each do |c| %>
			<%= render "customs/smallTile", :c => c %>
		<% end %>
	<% elsif @custom_count == 5 %>
		<%= render 'smallMenu' %>
		<% @sorted_custom_displays.each do |c| %>
			<%= render "customs/smallTile", :c => c %>
		<% end %>
		<%= render 'smallBrandingTile' %>
	<% elsif @custom_count == 4 %>
		<% @sorted_custom_displays.each do |c| %>
			<%= render "customs/largeTile", :c => c %>
		<% end %>
	<% else %>
		<% @sorted_custom_displays.each do |c| %>
		<%= render 'bigMenu' %>
			<%= render "customs/largeTile", :c => c %>
		<% end %>
		<%= render 'largeBrandingTile' %>
	<% end %>
</div>